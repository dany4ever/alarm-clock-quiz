<quiz>
    <question>
        What is the derivative of f(x), if f(x)=x^2
        <answer correct="true">f'(x)=2x</answer>
        <answer>f'(x)=(1/3)(x^3)</answer>
        <answer>f'(x)=2x^2</answer>
        <answer>f'(x)=2</answer>
    </question>
    <question>
        What type of acid is phosphoric acid?
        <answer correct="true">Weak, triprotic acid</answer>
        <answer>Strong, monoprotic acid</answer>
        <answer>Strong, diprotic acid</answer>
        <answer>Weak, monoprotic acid</answer>
    </question>
    <question>
        If a mass of 2kg is moving with a velocity of 5m/s, what is its Kinetic Energy?
        <answer correct="true">5J</answer>
        <answer>10J</answer>
        <answer>2.5J</answer>
        <answer>.5J</answer>
    </question>
</quiz>
